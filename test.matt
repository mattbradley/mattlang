fn fizzbuzz(n: Int) -> String
  if num % 15 == 0
    "fizzbuzz"
  elsif num % 3 == 0
    "fizz"
  elsif num % 5 == 0
    "buzz"
  else
    to_string(num)
  end
end

1..100
  |> Enum.map(&fizzbuzz)
  |> IO.puts

