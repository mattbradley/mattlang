fn puts(x: Number | String | Bool | Nil) -> Nil
  IO.puts(x)
end

module IO
  foreign fn puts(n: Number) -> Nil = mt_puts_n
  foreign fn puts(s: String) -> Nil = mt_puts_s

  fn puts(b: Bool) -> Nil
    if b
      puts("true")
    else
      puts("false")
    end
  end

  fn puts(n: Nil) -> Nil
    puts("")
  end
end
